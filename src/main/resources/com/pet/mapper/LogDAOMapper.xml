<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pet.demo.dao.LogDao">
    <select id="findALL" resultType="Log">
        select * from t_log
    </select>
    <!--保存员工-->
    <insert id="save" parameterType="Log">
        insert into t_log values(#{log_id},#{log_time},#{log_detail})
    </insert>
    <select id="findFive" resultType="Log">
        select * from t_log ORDER BY log_id DESC LIMIT 5
    </select>
</mapper>